#!/usr/bin/env bash

# 1st argument is the experiment (e.g. three_body_spring_mass) 2nd argument is the task (evaluate, plot or full)
python -m hyperverlet.main --config-path configurations/integrator_experiments/$1/euler.json $2 &
python -m hyperverlet.main --config-path configurations/integrator_experiments/$1/hypereuler.json $2 &
python -m hyperverlet.main --config-path configurations/integrator_experiments/$1/hyperverlet.json $2 &
python -m hyperverlet.main --config-path configurations/integrator_experiments/$1/velocityverlet.json $2